printf("1-Registrarse\n");
printf("2-Inicio de Sesión.\n");
printf("3-Ver Cartelera\n");
printf("4-Salir del Programa\n");
scanf ("%d",&opcion);
switch (opcion)
{
case 1:printf ("\n");
break;
case 2:printf ("\n");
break;
case 3:printf ("\n");
break;
case 4:printf ("\n");
break;
default:
 printf ("Opcion incorrecta\n");
}
}
